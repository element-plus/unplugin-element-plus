{
  "name": "unplugin-element-plus",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "lint": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx && prettier --check .",
    "lint:fix": "eslint --fix ./src --ext .vue,.js,.ts,.jsx,.tsx && prettier --write .",
    "build": "rimraf dist && tsup src/*.ts --format cjs,esm --dts && esno scripts/postbuild.ts",
    "vite:build": "npm -C examples/vite run build",
    "vite:dev": "npm -C examples/vite run dev"
  },
  "keywords": [
    "element-plus",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "plugin"
  ],
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/element-plus/unplugin-element-plus"
  },
  "bugs": {
    "url": "https://github.com/element-plus/unplugin-element-plus/issues"
  },
  "homepage": "https://github.com/element-plus/unplugin-element-plus/tree/main/#readme",
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "es-module-lexer": "^0.7.1",
    "magic-string": "^0.25.7",
    "unplugin": "^0.2.7"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.9.1",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.0",
    "tsup": "^4.14.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "vue": "2 || 3"
  },
  "engines": {
    "node": ">=14"
  }
}
